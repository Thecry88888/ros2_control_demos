<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="lrmate_200id_description">

  <!-- helper macro -->
  <xacro:include filename="$(find ros2_control_demo_description)/lrmate_200id/urdf/inc/create_link.xacro"/>

  <xacro:create_link
      link_name="base_link"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_0"
      link_mass="4.8532"
      ixx="0.02198"
      ixy="0.0001185"
      ixz="0.0001430"
      iyy="0.02382"
      iyz="0.0001"
      izz="0.02280"
  />

  <xacro:create_link
      link_name="link_1"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_1"
      link_mass="14.3548"
      ixx="0.9059"
      ixy="-0.0006305"
      ixz="-0.1324"
      iyy="0.9590"
      iyz="-0.0008315"
      izz="0.1326"
  />

  <xacro:create_link
      link_name="link_2"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_2"
      link_mass="6.3613"
      ixx="0.10665127657"
      ixy="-0.00008067"
      ixz="-0.001451"
      iyy="0.0870"
      iyz="0.0005574"
      izz="0.03170"
  />

  <xacro:create_link
      link_name="link_3"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_3"
      link_mass="2.3942"
      ixx="0.00633472848658925"
      ixy="-4.9504E-05"
      ixz="-0.0007613"
      iyy="0.007328"
      iyz="-6.6472E-05"
      izz="0.007260"
  />

  <xacro:create_link
      link_name="link_4"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_4"
      link_mass="1.5"
      ixx="0.005"
      ixy="0"
      ixz="0"
      iyy="0.005"
      iyz="0"
      izz="0.005"
  />

  <xacro:create_link
      link_name="link_5"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_5"
      link_mass="1.2"
      ixx="0.002"
      ixy="0"
      ixz="0"
      iyy="0.002"
      iyz="0"
      izz="0.002"
  />

  <xacro:create_link
      link_name="link_6"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_demo_description/lrmate_200id"
      mesh_name="link_6"
      scale_factor="0.025"
      link_mass="0.09321"
      ixx="5.2654E-05"
      ixy="3.9145E-14"
      ixz="3.4366E-08"
      iyy="3.1586E-05"
      iyz="2.7569E-11"
      izz="3.1628E-05"
  />

  <link name="ft_frame"/>
  <link name="tool0"/>

  <!-- base_joint fixes base_link to the environment -->
  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- joints - main serial chain -->
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.042741" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <!-- according to official specs ${340.0 / 2.0 * pi / 180.0}-->
    <limit effort="16.6" lower="-2.9671" upper="2.9671" velocity="5.93"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0.05 0 0.28726" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="16.6" lower="-1.7453" upper="2.5307" velocity="4.28"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 0.33" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit effort="16.6" lower="-1.2217" upper="3.5779" velocity="7.33"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0.088001 0 0.035027" rpy="0 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit effort="16.6" lower="-3.3161" upper="3.3161" velocity="6.63"/>
  </joint>

  <joint name="joint_5" type="revolute">
    <parent link="link_4"/>
    <child link="link_5"/>
    <origin xyz="0.2454 0 0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit effort="16.6" lower="-2.1817" upper="2.1817" velocity="4.36"/>
  </joint>

  <joint name="joint_6" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="0.05 0 0" rpy="1.5708 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit effort="9.4" lower="-6.2832" upper="6.2832" velocity="12.57"/>
  </joint>

  <joint name="joint-ft_frame" type="fixed">
    <parent link="link_6"/>
    <child link="ft_frame"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <joint name="joint-tool" type="fixed">
    <parent link="ft_frame"/>
    <child link="tool0"/>
    <origin xyz="0 0 .185" rpy="0 0 0"/>
  </joint>

</xacro:macro>

</robot>
